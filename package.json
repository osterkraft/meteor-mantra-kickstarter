{
  "name": "meteor-mantra-kickstarter",
  "version": "0.2.1",
  "scripts": {
    "lint": "eslint . --ext .js --ext .jsx",
    "lintfix": "npm run lint -- --fix",
    "testonly": "mocha .scripts/mocha_boot.js client/**/tests/**/*.js --compilers js:babel-core/register",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js,jsx",
    "acceptance": "./node_modules/.bin/chimp .e2e_tests/chimp-config.js --ddp=http://localhost:3000 --watch --path=.e2e_tests",
    "workarounds": ".workarounds/fix.sh",
    "dsstore": "find . -name '*.DS_Store' -type f -delete"
  },
  "devDependencies": {
    "babel-core": "6.x.x",
    "babel-eslint": "6.x.x",
    "babel-plugin-react-require": "2.x.x",
    "babel-polyfill": "6.x.x",
    "babel-preset-es2015": "6.x.x",
    "babel-preset-react": "6.x.x",
    "babel-preset-stage-2": "6.x.x",
    "babel-root-slash-import": "1.x.x",
    "chai": "3.x.x",
    "enzyme": "^2.3.0",
    "eslint": "^2.13.1",
    "eslint-plugin-react": "^5.2.2",
    "mocha": "2.x.x",
    "react-addons-test-utils": "~15.2.0",
    "sinon": "1.17.x",
    "jsdom": "9.x.x"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "domready": "^1.0.8",
    "formsy-react": "0.18.1",
    "formsy-react-components": "0.8.1",
    "lodash": "^4.13.1",
    "logatim": "^0.9.0",
    "mantra-core": "^1.7.0",
    "meteor-node-stubs": "^0.2.3",
    "nodemailer": "^2.4.2",
    "nodemailer-mailgun-transport": "^1.2.0",
    "react": "~15.2.0",
    "react-dom": "~15.2.0",
    "react-komposer": "1.11.0",
    "react-mounter": "^1.0.0",
    "react-no-ssr": "^1.1.0",
    "react-select": "1.0.0-beta13",
    "react-simple-di": "1.2.0",
    "react-tagsinput": "3.9.0",
    "tcomb-form": "0.9.8"
  },
  "private": true
}
